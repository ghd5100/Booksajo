	//현재페이지 값 정하기
	function currentPageSelect(val, sunbun){
		if(sunbun < 1){
			gubun = "first";
			currentPage1 = 1;
		}else if(val == "pre"){
			if(sunbun == 1){
				gubun = "first";
				currentPage1 = sunbun;
			}else if(sunbun == "2"){	//첫페이지로 넘어올 경우
				gubun = "first";
				currentPage1 = sunbun - 1;
			}else{
				gubun = "normal";
				currentPage1 = sunbun - 2;
				currentPage2 = sunbun - 1;
			}
		}else if(val == "next"){
			if((sunbun == '26') || (Number(sunbun) + 1 == '26')){
				if(26 == 2){	//이미지 총 갯수가 2일 경우
					if(sunbun == 1){
						gubun = "last";
						currentPage1 = Number(sunbun) + 1;
						currentPage2 = Number(sunbun) + 2;	
					}
				}else if(26 == 1){	//이미지 총 갯수가 1일 경우
					gubun = "first";
				}
			}else if((Number(sunbun) + 2 == '26') || (Number(sunbun) + 3 == '26')){		//마지막페이지로 넘어갈 경우
				if(26 == 3){	//이미지 총 갯수가 3일 경우
